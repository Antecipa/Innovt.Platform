<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>Innovt.Cloud.AWS.SQS</PackageId>
    <PackageVersion>5.0.7</PackageVersion>
    <Authors>Michel Borges</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>Innovt Tecnologia</Copyright>
    <Description>AWS implementation for SQS Abstraction.</Description>
    <LangVersion>latest</LangVersion><AnalysisMode>AllEnabledByDefault</AnalysisMode>
  </PropertyGroup>
 <ItemGroup>
      <PackageReference Include="AWSSDK.SQS" Version="3.7.0.44" />
      <PackageReference Include="System.Diagnostics.DiagnosticSource" Version="5.0.1" />
      <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="5.0.3">
        <PrivateAssets>all</PrivateAssets>
        <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Innovt.Cloud.AWS\Innovt.Cloud.AWS.csproj" />
  </ItemGroup>

</Project>
